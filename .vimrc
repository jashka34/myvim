syntax on
colorscheme elflord
set number
set numberwidth=4
inoremap jj <Esc>
set fileformat=unix
set fileencoding=utf-8
set encoding=utf-8
set termencoding=utf-8
set keymap=russian-jcukenwin
set iminsert=0
set imsearch=0
set ruler
set hlsearch
set shiftwidth=4
highlight lCursor guifg=NONE guibg=Cyan
" nmap <Tab> <C-W><C-W>
nmap <Tab>l <C-W>l
nmap <Tab>k <C-W>k
nmap <Tab>j <C-W>j
nmap <Tab>h <C-W>h
